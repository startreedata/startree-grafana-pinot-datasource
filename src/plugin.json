      "time-series",
      "logs",
      "apache-pinot",
      "olap",
      "real-time-analytics"
    ],
    "logos": {
      "small": "img/startree-logo.svg",
      "large": "img/startree-logo.svg"
    },
    "links": [
      {
        "name": "Documentation",
        "url": "https://github.com/startreedata/startree-grafana-pinot-datasource/blob/main/README.md"
      },
      {
        "name": "Testing Guide",
        "url": "https://github.com/startreedata/startree-grafana-pinot-datasource/blob/main/TESTING.md"
      },
      {
        "name": "Support",
        "url": "https://github.com/startreedata/startree-grafana-pinot-datasource/issues"
