    "@grafana/ui": "10.4.2",
    "@lezer/common": "^1.2.1",
    "@lezer/highlight": "^1.2.0",
    "@lezer/lr": "^1.4.2",
    "@prometheus-io/codemirror-promql": "^0.55.0",
    "prismjs": "^1.29.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-select": "^5.2.1",
    "react-use": "^17.3.1",
    "react-virtualized-auto-sizer": "^1.0.6",
    "rxjs": "7.8.0",
