{
  "name": "pinot",
  "version": "1.0.12",
  "description": "Pinot connector for Grafana.",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "server": "docker compose up grafana --build",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "dev:backend": "yarn run build && mage -v && yarn run server",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "test": "jest --passWithNoTests --maxWorkers 4",
    "test:ci": "yarn run typecheck && yarn run lint && yarn run test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:codegen": "playwright codegen",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "Startree",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/e2e": "^11.0.2",
    "@grafana/e2e-selectors": "10.4.12",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/plugin-e2e": "^1.14.2",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@playwright/test": "^1.41.2",
    "@swc/core": "^1.3.62",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "12.1.5",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "copy-webpack-plugin": "^11.0.0",
    "crypto-random-string": "^5.0.0",
    "css-loader": "^6.7.3",
    "cypress": "^13.14.2",
    "dotenv": "^16.4.7",
    "eslint": "8.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-jsdoc": "^48.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mocha": "^8.4.0",
    "prettier": "^3.0.3",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.17.0",
